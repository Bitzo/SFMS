doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    title #{title}
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(href='assets/css/bootstrap.min.css', rel='stylesheet')
    link(rel='stylesheet',href='css/wechat/wechatShowProduct.css')
body(ng-app='myApp', ng-controller='wechatProductController')
  div.login
    div.container.col-sm-8.col-sm-offset-2.col-md-6.col-md-offset-3
      h2.form-signin-heading 商品展示
      div#product 123
      form(class='form-signin' name='signUpForm' method='post' role='form')
        div.form-group
          button.btn.btn-lg.btn-primary.pull-right(type="submit" ng-click="") 提交订单
script(src='assets/js/jquery-2.0.3.min.js')
script(src='assets/js/bootstrap.min.js')
script(src='/bower_components/angular/angular.min.js')
script(src='/js/angularjs/controller/controllers.js')